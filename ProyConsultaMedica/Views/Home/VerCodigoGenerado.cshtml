
@{
    ViewBag.Title = "VerCodigoGenerado";
    Layout = "~/Views/Shared/_LayoutPrincipal.cshtml";
}


    <!-- Section: inner-header -->
<section class="inner-header divider parallax layer-overlay overlay-white-8" data-bg-img="../images/bg/bg6.jpg">
    <div class="container pt-60 pb-60">
        <!-- Section Content -->
        <div class="section-content">
            <div class="row">
                <div class="col-md-12 text-center">
                    <h2 class="title">Su codigo Generado es</h2>
                    <ol class="breadcrumb text-center text-black mt-10">
                        <li class="active text-theme-colored">@ViewBag.codigoGenerado</li>
                    </ol>
                </div>
            </div>
        </div>
    </div>
</section>


<p>
    @Html.ActionLink("Ver Consulta Realizada", "VerConsulta")
    @Html.ActionLink("Salir", "Index")

</p>